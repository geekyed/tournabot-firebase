    "node-fetch": "^2.6.0",
    "standard": "^12.0.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "eslint": "^5.12.0",
    "eslint-plugin-promise": "^4.0.1",
    "firebase-functions-test": "^0.1.6"
  },
  "private": true
}
